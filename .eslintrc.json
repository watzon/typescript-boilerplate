{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "warnOnUnsupportedTypeScriptVersion": false,
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "browser": false,
        "node": true,
        "es6": true,
        "jest/globals": true
    },
    "plugins": [
        "jsdoc", "jest"
    ],
    "extends": [
      "standard",
      "plugin:jest/recommended"
    ]
}
